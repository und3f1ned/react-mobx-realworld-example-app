{
  "name": "react-mobx-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@rescripts/cli": "0.0.13",
    "@rescripts/rescript-env": "0.0.11",
    "@rescripts/rescript-use-rewire": "0.0.5",
    "chalk": "^3.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "fs": "0.0.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react-app-rewire-micro-frontends": "0.0.1",
    "react-scripts": "3.2.0",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "dependencies": {
    "antd": "^3.26.3",
    "history": "4.10.1",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "promise.prototype.finally": "^3.1.2",
    "query-string": "6.9.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "5.1.2",
    "superagent": "^5.1.2",
    "superagent-promise": "^1.1.0"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --color -f stylish --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --color -r -f verbose --fix",
      "git add"
    ]
  },
  "scripts": {
    "start-js": "HTTPS=true rescripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && rescripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "rescripts test --env=jsdom",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint ./src/** --ext .jsx,.js --color -f stylish --fix",
    "lint:css": "stylelint './src/**/*.scss' --color -r -f verbose --fix",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx}'",
    "release": "npm version patch && git push --tags && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./index.js ${HUSKY_GIT_PARAMS}",
      "pre-commit": "git add . -A && lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
