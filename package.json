{
  "name": "react-mobx-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "chalk": "^2.4.2",
    "customize-cra": "^0.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "fs": "0.0.1-security",
    "husky": "^3.0.9",
    "react-scripts": "^3.2.0",
    "lint-staged": "^9.4.2",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "react-app-rewired": "^2.1.4",
    "stylelint": "^11.1.1",
    "stylelint-scss": "^3.12.0"
  },
  "dependencies": {
    "history": "4.10.1",
    "mobx": "^5.14.2",
    "mobx-react": "^6.1.4",
    "promise.prototype.finally": "^3.1.1",
    "query-string": "6.8.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "5.1.2",
    "superagent": "^5.1.0",
    "superagent-promise": "^1.1.0"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --color -f stylish --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --color -r -f verbose --fix",
      "git add"
    ]
  },
  "scripts": {
    "start-js": "HTTPS=true react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-rewired build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-app-rewired test --env=jsdom",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint ./src/** --ext .jsx,.js --color -f stylish --fix",
    "lint:css": "stylelint './src/**/*.scss' --color -r -f verbose --fix",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx}'",
    "release": "npm version patch && git push --tags && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./index.js ${HUSKY_GIT_PARAMS}",
      "pre-commit": "git add . -A && lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
