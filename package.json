{
  "name": "react-mobx-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "custom-react-scripts": "0.2.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fs": "0.0.1-security",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-scss": "^3.9.3"
  },
  "dependencies": {
    "history": "4.9.0",
    "marked": "^0.7.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.7.2",
    "query-string": "6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "5.0.1",
    "superagent": "^5.1.0",
    "superagent-promise": "^1.1.0"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --color -f stylish --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --color -r -f verbose --fix",
      "git add"
    ]
  },
  "scripts": {
    "start-js": "HTTPS=true react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint ./src/** --ext .jsx,.js --color -f stylish --fix",
    "lint:css": "stylelint './src/**/*.scss' --color -r -f verbose --fix",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx}'",
    "release": "npm version patch && git push --tags && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./index.js ${HUSKY_GIT_PARAMS}",
      "pre-commit": "git add . -A && lint-staged"
    }
  }
}
